{
  "name": "test2doc",
  "version": "0.4.2",
  "description": "Build API docs from your tests",
  "main": "lib/index.js",
  "scripts": {
    "unit-test": "jest ./lib",
    "integration-test": "jest ./spec",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "npm run lint && jest \"./(lib|spec)\"",
    "example": "mocha ./example/mocha/**/*.spec.js && jest ./example/jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackia/test2doc.js.git"
  },
  "keywords": [
    "docs",
    "documents",
    "api",
    "test"
  ],
  "author": "Stackia <jsq2627@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stackia/test2doc.js/issues"
  },
  "homepage": "https://github.com/stackia/test2doc.js#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "lint-staged": "^12.1.7",
    "mocha": "^10.0.0",
    "prettier": "^2.5.1",
    "should": "^13.2.3",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "clone": "^2.1.0",
    "js-yaml": "^4.1.0",
    "methods": "^1.1.2",
    "path-to-regexp": "^6.2.0",
    "symbol-description": "^1.1.0"
  },
  "engines": {
    "node": ">= 12.22.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "env": {
      "jest": true,
      "mocha": true
    },
    "rules": {
      "no-restricted-syntax": "off"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
