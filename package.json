{
  "name": "test2doc",
  "version": "0.4.0",
  "description": "Build API docs from your tests",
  "main": "lib/index.js",
  "scripts": {
    "unit-test": "cross-env TEST2DOC_ENV=test jest ./lib",
    "integration-test": "cross-env TEST2DOC_ENV=test jest ./spec",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "npm run lint && cross-env TEST2DOC_ENV=test jest \"./(lib|spec)\"",
    "example": "mocha ./example/mocha/**/*.spec.js && jest ./example/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackia/test2doc.js.git"
  },
  "keywords": [
    "docs",
    "documents",
    "api",
    "test"
  ],
  "author": "Stackia <jsq2627@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stackia/test2doc.js/issues"
  },
  "homepage": "https://github.com/stackia/test2doc.js#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^9.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.10",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "should": "^13.2.3",
    "supertest": "^5.0.0"
  },
  "dependencies": {
    "clone": "^2.1.0",
    "js-yaml": "^3.14.0",
    "methods": "^1.1.2",
    "path-to-regexp": "^6.1.0",
    "symbol-description": "^1.0.0"
  },
  "engines": {
    "node": ">= 10.14"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "standard",
      "plugin:prettier/recommended"
    ],
    "env": {
      "jest": true,
      "mocha": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
